using System;

class Program
{
    static void Main(string[] args)
    {
        // Leitura do valor inteiro X
        int X = int.Parse(Console.ReadLine());

        // Iterar de 1 até X
        for (int i = 1; i <= X; i++)
        {
            // Verificar se o número i é ímpar
            if (i % 2 != 0)
            {
                Console.WriteLine(i); // Imprimir o número ímpar
            }
        }
    }
}
