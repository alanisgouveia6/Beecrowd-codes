using System;

class Program
{
    static void Main(string[] args)
    {
        int X, Y;

        while (true)
        {
            string[] valores = Console.ReadLine().Split(' ');
            X = int.Parse(valores[0]);
            Y = int.Parse(valores[1]);

            if (X == Y)
                break;

            if (X < Y)
                Console.WriteLine("Crescente");
            else
                Console.WriteLine("Decrescente");
        }
    }
}
