using System;

class Program
{
    static void Main()
    {
        // Lê os dois valores inteiros
        int A = int.Parse(Console.ReadLine());
        int B = int.Parse(Console.ReadLine());
        
        // Calcula o produto
        int PROD = A * B;
        
        // Imprime a saída com a formatação correta
        Console.WriteLine($"PROD = {PROD}");
    }
}
